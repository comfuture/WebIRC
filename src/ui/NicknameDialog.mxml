<?xml version="1.0" encoding="utf-8"?>
<s:TitleWindow xmlns:fx="http://ns.adobe.com/mxml/2009" 
			   xmlns:s="library://ns.adobe.com/flex/spark" 
			   xmlns:mx="library://ns.adobe.com/flex/mx" width="300" title="Nickname" skinClass="spark.skins.spark.TitleWindowSkin">
	<s:layout>
		<s:VerticalLayout/>
	</s:layout>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<mx:Form width="100%" height="100%">
		<mx:FormItem label="Nickname" width="100%">
			<s:TextInput width="100%"/>
		</mx:FormItem>
		<s:Group width="100%">
			<s:layout>
				<s:HorizontalLayout/>
			</s:layout>
			<mx:Spacer width="100%" height="100%"/>
			<s:Button label="OK" skinClass="spark.skins.spark.DefaultButtonSkin"/>
			<s:Button label="Cancel"/>
		</s:Group>
	</mx:Form>
</s:TitleWindow>
